{
  "compilerOptions": {
    "lib": ["es6"], 
    "target": "es5", 
    "module": "commonjs", 
    "outDir": "dist/", 
    "rootDir": "src/", 
    "strict": true, 
    "allowSyntheticDefaultImports": true, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true, 
    "isolatedModules": true, 
    "downlevelIteration": true, 
  }
}